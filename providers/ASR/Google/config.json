{
  "provider": {
    "id": "google",
    "name": "Google Cloud Speech-to-Text",
    "description": "Google's cloud-based speech recognition service",
    "base_url": "https://speech.googleapis.com/v1",
    "requires_api_key": true,
    "api_key_type": "string",
    "icon_url": "https://cdn.jsdelivr.net/gh/devicons/devicon/icons/google/google-original.svg",
    "logo_url": "https://cloud.google.com/_static/cloud/images/social-icon-google-cloud-1200-630.png"
  },
  "models": [
    {
      "id": "latest_long",
      "name": "Latest Long Model",
      "description": "Latest model optimized for long-form audio",
      "supported_languages": ["hi-IN", "en-IN", "en-US", "en-GB", "es-ES", "fr-FR", "de-DE", "it-IT", "pt-BR", "ru-RU", "ja-JP", "ko-KR", "zh-CN", "ar-SA"],
      "features": ["Enhanced accuracy", "Long-form audio", "Automatic punctuation"]
    },
    {
      "id": "latest_short", 
      "name": "Latest Short Model",
      "description": "Latest model optimized for short-form audio",
      "supported_languages": ["hi-IN", "en-IN", "en-US", "en-GB", "es-ES", "fr-FR", "de-DE", "it-IT", "pt-BR", "ru-RU", "ja-JP", "ko-KR", "zh-CN", "ar-SA"],
      "features": ["Low latency", "Short-form audio", "Real-time"]
    },
    {
      "id": "command_and_search",
      "name": "Command and Search",
      "description": "Optimized for voice commands and search queries",
      "supported_languages": ["hi-IN", "en-IN", "en-US", "en-GB", "es-ES", "fr-FR", "de-DE", "it-IT", "pt-BR", "ja-JP", "ko-KR", "zh-CN"],
      "features": ["Voice commands", "Search queries", "Low latency"]
    }
  ],
  "supported_languages": ["hi-IN", "en-IN", "en-US", "en-GB", "es-ES", "fr-FR", "de-DE", "it-IT", "pt-BR", "ru-RU", "ja-JP", "ko-KR", "zh-CN", "ar-SA"]
}